<%
  pages = sitemap.resources.find_all.select { |page| page.destination_path =~ /\.html/ }
%>
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
<% pages.each do |page| %>
  <url>
    <loc>http://www.dartboard.io/<%= page.destination_path %></loc>
    <priority>0.7</priority>
  </url>
<% end %>
</urlset>
